{
  "manifest_version": 2,
 
  "name": "FeedCleaner",
  "version": "0.1.0",
  "description": "Remove Uneccesary Posts from FB Feed",
 
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://www.facebook.com/*"]
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
 
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}